<view class="container combat-list">
    <block wx:for="{{combat}}">
        <view class="image-section">
            <image style="width:80px;height:80px;background-color:#eeeeee" mode="aspectFit" src="{{item.src}}" />
        </view>
        <text>{{item.score}} {{item.time}}</text>
    </block>
</view>